import 'package:flutter/material.dart';
import 'package:firstfluproject/pages/createaccount.dart';
import 'package:firstfluproject/pages/welcomepage.dart';
import 'package:firstfluproject/pages/dashboard.dart';
import 'package:firstfluproject/pages/viewprofile.dart';
import 'package:firstfluproject/pages/viewcourses.dart';
import 'package:firstfluproject/pages/flutterbasics.dart';
void main(){
  runApp(MyApp());
}
class MyApp extends StatelessWidget {
  const MyApp({super.key});

  @override
  Widget build(BuildContext context) {
    return MaterialApp(
      debugShowCheckedModeBanner: false,
      initialRoute: '/',
      routes:{
        '/': (context)=> welcomepage(),
        '/createaccount': (context)=> createaccount(),
        '/dashboard': (context)=> dashboard(),
        '/viewprofile': (context)=> viewprofile(),
        '/viewcourses': (context)=> viewcourses(),
        '/flutterbasics': (context)=> flutterbasics(),


      }
    );
  }
}












      


    
